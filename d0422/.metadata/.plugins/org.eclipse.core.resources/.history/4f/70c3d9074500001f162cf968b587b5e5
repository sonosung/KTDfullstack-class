package q128;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int cnt = 0;

		for (;;) {

			for (int i = 0; i < cnt; i++) {
				if (num % 3 != 0 || num % 5 != 0) {
					cnt++;
					cnt = i++;
				}
			}
			break;
		}
		sc.close();
		System.out.println(cnt);
	}

}
