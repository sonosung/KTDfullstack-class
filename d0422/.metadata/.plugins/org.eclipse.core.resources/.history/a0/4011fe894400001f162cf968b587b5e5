package q128;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int cnt = 0;
		sc.close();
		for (;;) {
			if (num % 3 != 0 || num % 5 != 0) {
				cnt++;
				System.out.println(cnt);
			}
			break;
		}
		
	}

}
