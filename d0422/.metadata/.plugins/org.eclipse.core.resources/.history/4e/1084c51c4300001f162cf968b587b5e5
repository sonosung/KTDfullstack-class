package q128;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();

		for(int i = 0; i <= num; i++) {
			int b = i;
			System.out.println(b + " ");
		}
		
		sc.close();
	}

}
