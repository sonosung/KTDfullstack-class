package q128;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int cnt = 0;
		sc.close();
		while (true) {
			int num = sc.nextInt();
//			System.out.println(num);
			if (num % 3 != 0 || num % 5 != 0) {
				cnt++;
			}
			if (num == 0) {
				System.out.println(cnt);
			}

		}
		
	}
}
