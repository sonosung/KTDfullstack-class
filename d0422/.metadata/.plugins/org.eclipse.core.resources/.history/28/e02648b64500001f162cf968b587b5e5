package q128;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();

		for (;;) {
			int cnt = 0;
			for (int i = 0; i < num; i++) {
				if (num % 3 == 0 || num % 5 == 0) {
					cnt = i++;
				} else if (num == 0) {
					System.out.println(cnt);
				}
			}
			sc.close();
			
		}
		
	}

}
